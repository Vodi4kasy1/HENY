local aimbotEnabled = false
local targetPart = "Head"  -- По умолчанию цель — голова
local aimbotKey = Enum.KeyCode.Z  -- По умолчанию клавиша для включения/выключения аимбота

-- Переключатель аимбота
Section1488:Toggle({
    text = "Enable Aimbot",
    state = aimbotEnabled,
    callback = function(state)
        aimbotEnabled = state
        print("Aimbot enabled:", aimbotEnabled)
    end
})

-- Дропдаун для выбора целевой части (голова или торс)
Section1488:Dropdown({
    text = "Target Part",
    list = {"Head", "Torso"},
    default = "Head",
    callback = function(selected)
        targetPart = selected
        print("Target part set to:", targetPart)
    end
})

-- Клавиша для переключения аимбота
Section1488:Keybind({
    text = "Toggle Aimbot Key",
    default = aimbotKey,
    callback = function(key)
        aimbotEnabled = not aimbotEnabled  -- Переключение состояния аимбота
        print("Aimbot toggled. Enabled:", aimbotEnabled)
    end
})

-- Переменные для FOV круга
local fovEnabled = false
local fovOutlineOnly = false
local fovCircleRadius = 100
local fovCircle = Drawing.new("Circle")
fovCircle.Visible = false
fovCircle.Color = Color3.new(1, 1, 1)
fovCircle.Thickness = 2
fovCircle.NumSides = 100
fovCircle.Radius = fovCircleRadius
fovCircle.Transparency = 1

-- Переключатель для отображения FOV круга
Section1487:Toggle({
    text = "Enable FOV Circle",
    state = fovEnabled,
    callback = function(state)
        fovEnabled = state
        fovCircle.Visible = fovEnabled
        print("FOV Circle enabled:", fovEnabled)
    end
})

-- Переключатель для outline в FOV круге
Section1487:Toggle({
    text = "Outline Only",
    state = fovOutlineOnly,
    callback = function(state)
        fovOutlineOnly = state
        fovCircle.Filled = not fovOutlineOnly
        print("FOV Circle outline only:", fovOutlineOnly)
    end
})

-- Слайдер для изменения радиуса FOV круга
Section1487:Slider({
    text = "FOV Circle Size",
    min = 10,
    max = 200,
    callback = function(size)
        fovCircle.Radius = size
        fovCircleRadius = size
        print("FOV Circle radius:", fovCircleRadius)
    end
})

-- Функция для получения ближайшего игрока
local function getClosestPlayer()
    local closestPlayer = nil
    local shortestDistance = math.huge  -- Начинаем с очень большого расстояния

    local Camera = game.Workspace.CurrentCamera
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild(targetPart) then
            local part = player.Character[targetPart]
            local screenPoint, onScreen = Camera:WorldToViewportPoint(part.Position)
            if onScreen then
                local distance = (Vector2.new(screenPoint.X, screenPoint.Y) - Vector2.new(game:GetService("UserInputService"):GetMouseLocation().X, game:GetService("UserInputService"):GetMouseLocation().Y)).Magnitude
                if distance < shortestDistance then
                    closestPlayer = player
                    shortestDistance = distance
                end
            end
        end
    end

    return closestPlayer
end

-- Логика аимбота
game:GetService("RunService").RenderStepped:Connect(function()
    if aimbotEnabled then
        local closestPlayer = getClosestPlayer()
        if closestPlayer and closestPlayer.Character then
            local target = closestPlayer.Character:FindFirstChild(targetPart) or closestPlayer.Character:FindFirstChild("UpperTorso")
            if target then
                local Camera = game.Workspace.CurrentCamera
                Camera.CFrame = CFrame.new(Camera.CFrame.Position, target.Position)
            end
        end
    end

    -- Обновление FOV круга
    if fovEnabled then
        local mouseLocation = game:GetService("UserInputService"):GetMouseLocation()
        fovCircle.Position = mouseLocation
    end
end)
